@using SellIt.Core.ViewModels.Messages
@model IEnumerable<AllProductMessagesViewModel>

@foreach (var item in Model)
{
    <table class="table">
        <thead>
            <tr>
                <th scope="col">User</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@item.UserName</td>
            </tr>
        <a asp-action="GetProductMessageById" asp-controller="Message" asp-route-id="@item.Id" class="btn btn-success">View</a>
        </tbody>
    </table>
}
