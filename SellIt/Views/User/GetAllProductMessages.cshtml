@using SellIt.Areas.User.ViewModels
@model IEnumerable<SendMessageViewModel>

@foreach (var item in Model)
{
    <table class="table">
        <thead>
            <tr>
                <th scope="col">User</th>
                <th scope="col">Message</th>
                <th scope="col">Product</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@item.UserName</td>
                <td>@item.Text</td>
                <td>@item.ProductId</td>
            </tr>
        </tbody>
    </table>
}
